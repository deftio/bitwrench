{
	"name": "bitwrench",
	"version": "1.2.5",
	"description": "A library for misc javascript functions.",
	"main": "bitwrench.js",
	"repository": {
		"type": "git",
		"url": "git://github.com/deftio/bitwrench.git"
	},
	"keywords": [
		"javascript miscellaneous helper functions"
	],
